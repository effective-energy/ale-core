    },
    "PublicKey": {
      "example": "WYJsP2L2883QBqEB6zKv4pD4i846QqNNREpudp5VWLY=",
      "pattern": "^(?:[A-Za-z0-9+/]{4})*(?:[A-Za-z0-9+/]{2}==|[A-Za-z0-9+/]{3}=)?$",
      "format": "byte",
      "type": "string"
    },
