        },
        "MsgEmployerAcceptance": {
          "$ref": "#/definitions/EmployerAcceptance"
        }
      },
      "maxProperties": 1,
      "minProperties": 1,
      "type": "object"
    },
    "JobOffer": {
      "required": [
        "descr",
        "price",
        "reqs"
      ],
      "properties": {
        "descr": {
          "$ref": "#/definitions/JobDescription"
        },
        "deadline": {
          "$ref": "#/definitions/UTCTime"
        },
        "price": {
          "$ref": "#/definitions/TokenCount"
        },
        "reqs": {
          "$ref": "#/definitions/Requirements"
        }
      },
      "type": "object"
    },
    "StorageReference": {
      "example": "103b512c203a303a20413b4b483d3e203c353d4f3f",
      "pattern": "^[a-z0-9]*$",
      "format": "base16",
      "type": "string"
    },
    "UTCTime": {
      "example": "2016-07-22T00:00:00Z",
      "format": "yyyy-mm-ddThh:MM:ssZ",
      "type": "string"
    },
    "Tokens": {
      "properties": {
        "$@": {
          "$ref": "#/definitions/TokenCount"
        }
      },
      "type": "object"
    },
    "TokenCount": {
      "example": 10,
      "maximum": 18446744073709552000,
      "minimum": 0,
      "type": "integer"
    },
    "Requirements": {
      "description": "`RConstant` – a constant value that can be used in expressions described below.\n`RVar` – reference to an input parameter.\n`RLess` – operator `<` for integers.\n`RAnd` – logical `and` of a list of booleans.\n`RVerify` – whether the bytes are signed by a valid signature.",
      "properties": {
        "RConst": {
          "$ref": "#/definitions/Constant"
        },
        "RVar": {
          "maximum": 9223372036854776000,
          "minimum": -9223372036854776000,
          "type": "integer"
        },
        "RAnd": {
          "items": {
            "$ref": "#/definitions/Requirements"
          },
          "type": "array"
        },
        "RLess": {
          "minItems": 2,
          "items": [
            {
              "$ref": "#/definitions/Requirements"
            },
            {
              "$ref": "#/definitions/Requirements"
            }
          ],
          "maxItems": 2,
          "type": "array"
        },
        "RVerify": {
          "minItems": 3,
          "items": [
            {
              "$ref": "#/definitions/Requirements"
            },
            {
              "$ref": "#/definitions/Requirements"
            },
            {
              "$ref": "#/definitions/Requirements"
            }
          ],
          "maxItems": 3,
          "type": "array"
        },
        "RSha256": {
          "$ref": "#/definitions/Requirements"
        },
        "REqBs": {
          "minItems": 2,
          "items": [
            {
              "$ref": "#/definitions/Requirements"
            },
            {
              "$ref": "#/definitions/Requirements"
            }
          ],
          "maxItems": 2,
          "type": "array"
        }
      },
      "maxProperties": 1,
      "minProperties": 1,
      "type": "object"
    },
    "Constant": {
      "properties": {
        "RInt": {
          "maximum": 9223372036854776000,
